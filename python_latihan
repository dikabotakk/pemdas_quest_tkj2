a =int(input("masukan nilai a:"))
b =float(input("masukan nilai b:"))
c= a+b

print("A=" ,a)
print("B=" ,b)
print(a,"+",b,"=",c)

x=int(input("masukan suhu celcius: "))
y=(9/5*x) + 32
print(x,"celcius=" ,y, "fahrenheit")